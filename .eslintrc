{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "plugins": ["@typescript-eslint", "prettier"],
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended",
    "prettier"
  ],
  "rules": {
    "no-console": 0,
    "prettier/prettier": 2,
    "@typescript-eslint/explicit-module-boundary-types": 0 //! TODO: update it
  },
  "env": {
    "browser": true,
    "node": true
  }
}
